{
  "name": "the-human-funnel",
  "version": "1.0.0",
  "description": "Get the best applicants from 10k to under 100 autonomously filtered.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build && npm run build:frontend",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:frontend": "cd frontend && npm start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:integration:watch": "jest --config jest.integration.config.js --watch",
    "test:all": "npm run test && npm run test:integration",
    "clean": "rimraf dist",
    "install:frontend": "cd frontend && npm install",
    "migrate": "ts-node scripts/migrate.ts",
    "migrate:down": "ts-node scripts/migrate.ts down",
    "migrate:status": "ts-node scripts/migrate.ts status",
    "seed": "ts-node scripts/seed.ts",
    "seed:demo": "ts-node scripts/seed.ts --demo",
    "seed:clear": "ts-node scripts/seed.ts --clear",
    "deploy": "./scripts/deploy.sh",
    "deploy:dev": "./scripts/deploy.sh -e development",
    "deploy:prod": "./scripts/deploy.sh -e production",
    "backup": "./scripts/backup.sh",
    "restore": "./scripts/restore.sh",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bull": "^4.10.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.1.1",
    "mongodb-memory-server": "^10.1.2",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.60.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@google/generative-ai": "^0.24.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-data-grid": "^8.11.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^2.0.0",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "mongoose": "^8.18.0",
    "multer": "^2.0.2",
    "openai": "^5.16.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.17.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-scripts": "^5.0.1",
    "redis": "^5.8.2",
    "uuid": "^11.1.0"
  }
}
